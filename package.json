{
  "name": "coffeekraken-s-atv-card-component",
  "version": "1.0.0",
  "description": "Create a nice apple tv card effect with a cool dynamic reflection effect.",
  "main": "dist/index.js",
  "homepage": "http://coffeekraken.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekraken/s-atv-card-component"
  },
  "scripts": {
    "dist": "npm run dist.js",
    "dist.js": "babel src -d dist",
    "dist.watch": "concurrently --kill-others \"npm run dist.watch.js\"",
    "dist.watch.js": "nodemon -w src -e js -x \"npm run dist.js\"",
    "demo": "coffeekraken-code-playground",
    "doc": "rm -rf doc && npm run doc.js",
    "doc.js": "coffeekraken-docblock-to-markdown -f 'src/**/*.js' -d doc --remove-path src/",
    "test": "coffeekraken-testing-stack",
    "start": "concurrently --kill-others \"npm run dist.watch\" \"npm run demo\""
  },
  "keywords": [
    "atv",
    "apple",
    "tv",
    "appletv",
    "card",
    "shine",
    "effect",
    "parallax",
    "webcomponent",
    "component",
    "html",
    "sugar",
    "read",
    "more",
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [{
    "name": "Olivier Bossel",
    "email": "olivier.bossel@gmail.com",
    "url": "http://olivierbossel.com"
  }],
  "license": "MIT",
  "dependencies": {
    "coffeekraken-sugar": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "coffeekraken-docblock-to-markdown": "0.0.2",
    "coffeekraken-code-playground": "0.0.2",
    "coffeekraken-testing-stack": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "webcomponents.js": "^0.7.23",
    "webpack": "^1.14.0"
  }
}
